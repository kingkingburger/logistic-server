  "scripts": {
    "build": "nest build",
    "startpm": "pm2 start ecosystem.config.js",
    "start:all": "npm-run-all --parallel build startpm",
    "monit": "pm2 monit",
    "reload": "pm2 reload 0",
    "stop": "pm2 stop 0",
    "reload:all": "npm-run-all --parallel build reload",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "startb": "nest start -b swc",
    "startbw": "nest start -b swc -w",
    "schedule": "nest start -b swc --port 3002",
    "start:dev-backup": "nest start --watch",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "sim": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
